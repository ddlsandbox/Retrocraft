{
  "multipart": [
      { "when": { "facing": "up" },
                  "apply": { "model": "retrocraft:telepipe" }
      },
      { "when": { "facing": "down" },
                  "apply": { "model": "retrocraft:telepipe" }
      },
      { "when": { "facing": "south" },
                  "apply": { "model": "retrocraft:telepipe", "x": 90 }
      },
      { "when": { "facing": "north" },
                  "apply": { "model": "retrocraft:telepipe", "x": 90 }
      },
      { "when": { "facing": "east" },
                  "apply": { "model": "retrocraft:telepipe", "x": 90, "y": 90 }
      },
      { "when": { "facing": "west" },
                  "apply": { "model": "retrocraft:telepipe", "x": 90, "y": 90 }
      }
  ],
  "inventory": {
	 "model": "retrocraft:telepipe",
	    	"transform": "forge:default-block"
  }
}
